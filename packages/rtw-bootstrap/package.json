{
  "name": "rtw-bootstrap",
  "version": "0.0.1",
  "description": "rtw-bootstrap",
  "repository": {
    "type": "git",
    "url": "https://github.com/wx-chevalier/fe-boilerplates"
  },
  "license": "MIT",
  "keywords": [
    "webpack",
    "react"
  ],
  "author": "wx-chevalier@github",
  "prettier": {
    "printWidth": 100,
    "singleQuote": true
  },
  "lint-staged": {
    "*.{ts,tsx,scss,less,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "scripts": {
    "analyze": "npm run stats && webpack-bundle-analyzer dist/stats.json",
    "clean": "rimraf dist",
    "dev": "webpack-dev-server --config ./scripts/webpack/webpack.config.dev.js --hot",
    "build": "npm run build:prod",
    "build:prod": "NODE_ENV=production webpack --config ./scripts/webpack/webpack.config.prod.js -p",
    "start": "webpack-dashboard -- npm run dev",
    "stats": "webpack --config ./scripts/webpack.config.prod.js --mode production --profile --json > dist/stats.json"
  },
  "dependencies": {
    "rtw-core": "*",
    "systemjs": "^6.1.4"
  },
  "devDependencies": {
    "@wx-fc/app-config": "^0.1.37",
    "webpack": "^4.41.2"
  },
  "files": [
    "dist/"
  ]
}
