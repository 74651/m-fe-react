{
  "author": "wx-chevalier@github",
  "description": "rtw-host-app",
  "license": "MIT",
  "keywords": [
    "webpack",
    "react"
  ],
  "name": "rtw-host-app",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/wx-chevalier/fe-boilerplates"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "types": "dist/types/index.d.ts",
  "prettier": {
    "printWidth": 100,
    "singleQuote": true
  },
  "lint-staged": {
    "*.{ts,tsx,scss,less,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "scripts": {
    "prebuild": "npm run clean && npm run cleanCov",
    "clean": "rimraf dist/*",
    "cleanCov": "rimraf @coverage",
    "build": "NODE_ENV=production webpack --config ./scripts/webpack/webpack.config.prod.js",
    "build:umd": "NODE_ENV=production webpack --config ./scripts/webpack/webpack.config.umd.js",
    "dev": "webpack-dev-server --config ./scripts/webpack/webpack.config.dev.js --hot",
    "dev:umd": "webpack-dev-server --config ./scripts/webpack/webpack.config.umd.dev.js --hot",
    "lint": "tsc --noEmit && eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "test": "jest --config ../../scripts/jest/jest.config.js",
    "test:watch": "npm test -- --watch",
    "test:cov": "npm run cleanCov && npm test -- --coverage",
    "test:update": "npm test -- --updateSnapshot",
    "start": "npm run dev"
  },
  "dependencies": {
    "antd": "^3.21.2",
    "connected-react-router": "^6.5.2",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-fxxking-hooks": "^1.0.2",
    "react-redux": "^7.1.0",
    "redux-actions": "^2.6.5",
    "redux-promise-middleware": "^6.1.1",
    "redux-thunk": "^2.3.0",
    "rtw-components": "*",
    "rtw-core": "*",
    "smoothscroll-polyfill": "^0.4.4"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.9",
    "@types/enzyme": "^3.10.3",
    "@types/jest": "24.0.17",
    "@types/react-dom": "^16.8.5",
    "@types/react-redux": "^7.1.1",
    "@types/react-router-dom": "^4.3.4",
    "@types/redux-actions": "^2.6.1",
    "@types/smoothscroll-polyfill": "^0.3.1",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "@typescript-eslint/parser": "^1.7.0",
    "autoprefixer": "9.6.1",
    "awesome-typescript-loader": "^5.2.1",
    "classnames": "^2.2.6",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "3.1.0",
    "enzyme": "^3.10.0",
    "eslint": "^5.16.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-loader": "2.1.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.13.0",
    "file-loader": "4.1.0",
    "fork-ts-checker-webpack-plugin": "^1.5.0",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-template": "^6.2.0",
    "jest": "24.8.0",
    "jest-cli": "24.8.0",
    "json-server": "0.15.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.8.0",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "parallelshell": "^3.0.2",
    "postcss-loader": "3.0.0",
    "react-hot-loader": "^4.12.10",
    "react-router-dom": "^5.0.1",
    "resolve-url-loader": "3.1.0",
    "rimraf": "^2.6.3",
    "style-loader": "0.23.1",
    "ts-jest": "^24.0.2",
    "ts-loader": "6.0.4",
    "typescript": "3.5.3",
    "uglifyjs-webpack-plugin": "2.2.0",
    "url-loader": "^2.1.0",
    "wasm-loader": "^1.3.0",
    "webpack": "^4.39.1",
    "webpack-cli": "3.3.6",
    "webpack-dashboard": "3.0.7",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.2.1",
    "webpack-theme-color-replacer": "^1.2.17",
    "workerize-loader": "^1.0.4"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "files": [
    "dist/"
  ]
}
